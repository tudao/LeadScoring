<apex:page id="t" controller="cms.CoreController" standardStylesheets="false" showHeader="false" sidebar="false">
	
	<apex:composition template="{!page_template}">
		
		<apex:define name="header">
		
			<style type="text/css">
				body{
					font: normal 12px arial,sans-serif;
					margin: 10px;  
				}
				
				.sf-content h1{
					font: normal 18px arial,sans-serif;
				}
				
				.sf-content .sf-column1{
					float: left;
					margin-right: 10px;  
				}
				
				.sf-content .sf-column2{
					float: left;
					margin-right: 10px;  
				}
				
				.sf-content .sf-column3{
					float: left;
					margin-right: 10px;  
				}				
				
				.sf-content .sf-clear{
					clear: both; 
				}
				
				.top-corners{
					display: none;
				}	
				
				.bottom-corners{
					display: none; 
				}


            	table.ui-superform {
            		border: none; 	
            	}
            	
            	table.ui-superform td {
            		vertical-align: top;
            		padding: 5px;
            	}
            	
            	table.ui-superform td span{
            		margin: 0;
            		padding: 0; 
            	}
            	
            	table.ui-superform div.ui-superform {
            	
            	}	
				
				table.dt-table{
					padding: 0px; 
					margin: 0px;
					border: none;
					border-spacing: none;  	
				}
				
				table.dt-table tr.dt-row td{
					padding: 0px 0px 0px 0px;
					vertical-align: top;
					text-align: left;
				}

          		.ui-superform-input div{
          			padding:0px;
          			margin: 0px;
          		}
          		
          		.menu .active{
          			color: red;
          			font-weight: bold; 
          		}
          		
         	</style>		
        </apex:define>
		
		<apex:define name="body" >		
			<c:Panel panelName="column1" panelController="{!controller}" panelHeight="200px" />		
		</apex:define>	
	</apex:composition>

</apex:page>